    }
    
    
    function thisTime() public view returns(uint256 _thisTime) {
        _thisTime = now;
    }
    
    function finalization() internal {
        _token.transfer(_wallet, _token.balanceOf(this));
    }
    
    function transferWallet(address wallet) public onlyOwner {
        require(wallet != address(0));
        
        _wallet = wallet;
    }
    
    function tokenAmount() public view returns(uint256){
        return _token.balanceOf(this);
    }
    
    function forceFinalize() public onlyOwner {
        require(!isFinalized);
        
        _closingTime = block.timestamp;
        
        finalization();
        emit Finalized();
        
        isFinalized = true;
    }
}
